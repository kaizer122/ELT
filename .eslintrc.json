{
  "extends": ["airbnb-base/legacy" , "prettier"],
  "plugins": [ "node" , "prettier"],
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "prettier/prettier": 1,
    "no-console": 0,
    "no-underscore-dangle": 0
  }
}
